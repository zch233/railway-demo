{
    "name": "vue3-vite-template",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "serve": "vite",
        "serve:stage": "vite serve --mode staging",
        "serve:prod": "vite serve --mode production",
        "build": "vite build",
        "build:stage": "vite build --mode staging",
        "build:prod": "vite build --mode production",
        "preview": "yarn build && vite preview",
        "deploy": "gupo-deploy --mode staging -p",
        "deploy:prod": "gupo-deploy --mode production -p",
        "lint:prettier": "prettier --write . --loglevel warn --ignore-unknown",
        "lint:stylelint": "stylelint --fix \"**/*.{css,scss,sass,less,styl,html,vue}\" --cache --cache-location node_modules/.cache/stylelint/",
        "lint:eslint": "eslint --cache --max-warnings 0 src --fix --ext .ts,.tsx,.vue,.js,.jsx",
        "lint:ls-lint": "ls-lint",
        "commit": "git-cz",
        "pre-commit:lint-staged": "lint-staged",
        "prepare": "husky install"
    },
    "dependencies": {
        "ant-design-vue": "3.2.12",
        "axios": "0.26.1",
        "copy-to-clipboard": "^3.3.2",
        "dayjs": "1.11.5",
        "gupo-vue3-persistedstate": "0.0.7",
        "lodash": "^4.17.21",
        "nprogress": "^0.2.0",
        "pinia": "^2.0.16",
        "screenfull": "^6.0.2",
        "vue": "3.2.33",
        "vue-router": "4.0.14"
    },
    "devDependencies": {
        "@commitlint/cli": "16.2.3",
        "@commitlint/config-conventional": "16.2.1",
        "@ls-lint/ls-lint": "1.11.0",
        "@types/node": "17.0.18",
        "@typescript-eslint/eslint-plugin": "5.17.0",
        "@typescript-eslint/parser": "5.17.0",
        "@vitejs/plugin-legacy": "1.8.0",
        "@vitejs/plugin-vue": "2.3.1",
        "@vitejs/plugin-vue-jsx": "1.3.10",
        "autoprefixer": "10.4.2",
        "commitizen": "4.2.4",
        "cz-conventional-changelog": "3.3.0",
        "cz-customizable": "6.3.0",
        "eslint": "8.12.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-define-config": "1.3.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-vue": "8.5.0",
        "gupo-deploy": "1.1.0",
        "husky": "7.0.0",
        "less": "4.1.2",
        "lint-staged": "13.0.3",
        "postcss-html": "1.3.0",
        "postcss-less": "6.0.0",
        "prettier": "2.6.1",
        "rollup-plugin-visualizer": "5.5.4",
        "stylelint": "14.10.0",
        "stylelint-config-html": "1.0.0",
        "stylelint-config-prettier": "9.0.3",
        "stylelint-config-recommended-vue": "1.4.0",
        "stylelint-config-standard": "25.0.0",
        "stylelint-order": "5.0.0",
        "unplugin-auto-import": "^0.7.2",
        "unplugin-vue-components": "^0.19.5",
        "vite": "2.9.5",
        "vite-plugin-compression": "0.5.1",
        "vite-plugin-html": "3.0.6",
        "vite-plugin-mkcert": "1.6.0",
        "vite-plugin-svg-icons": "2.0.1",
        "vite-plugin-vue-setup-extend": "0.4.0",
        "vite-plugin-windicss": "1.8.4",
        "windicss": "3.5.1"
    },
    "resolutions": {
        "bin-wrapper": "npm:bin-wrapper-china"
    }
}
