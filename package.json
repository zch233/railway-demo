{
    "name": "vue3-vite-template",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "serve": "vite",
        "serve:stage": "vite serve --mode staging",
        "serve:prod": "vite serve --mode production",
        "build": "vite build",
        "build:stage": "vite build --mode staging",
        "build:prod": "vite build --mode production",
        "report": "rimraf dist && vite build",
        "preview": "yarn build && vite preview",
        "deploy": "gupo-deploy --mode staging -p",
        "deploy:prod": "gupo-deploy --mode production -p",
        "lint:prettier": "prettier --write . --loglevel warn --ignore-unknown",
        "lint:stylelint": "stylelint --fix \"**/*.{css,scss,sass,less,styl,html,vue}\" --cache --cache-location node_modules/.cache/stylelint/",
        "lint:eslint": "eslint --cache --max-warnings 0 src --fix --ext .ts,.tsx,.vue,.js,.jsx",
        "lint:ls-lint": "ls-lint",
        "commit": "git-cz",
        "pre-commit:lint-staged": "lint-staged",
        "prepare": "husky install"
    },
    "dependencies": {
        "@ant-design/icons-vue": "^6.1.0",
        "ant-design-vue": "3.2.13",
        "axios": "0.26.1",
        "copy-to-clipboard": "3.3.3",
        "dayjs": "1.11.6",
        "echarts": "5.4.0",
        "gupo-vue3-persistedstate": "0.0.7",
        "lodash": "4.17.21",
        "lodash-es": "4.17.21",
        "lodash-unified": "1.0.3",
        "nprogress": "0.2.0",
        "pinia": "2.0.24",
        "screenfull": "6.0.2",
        "vue": "3.2.45",
        "vue-router": "4.1.6",
        "xlsx": "0.18.5"
    },
    "devDependencies": {
        "@commitlint/cli": "16.2.3",
        "@commitlint/config-conventional": "16.2.1",
        "@ls-lint/ls-lint": "1.11.0",
        "@types/lodash": "4.14.190",
        "@types/lodash-es": "4.17.6",
        "@types/node": "18.11.9",
        "@typescript-eslint/eslint-plugin": "5.17.0",
        "@typescript-eslint/parser": "5.17.0",
        "@vitejs/plugin-legacy": "2.3.1",
        "@vitejs/plugin-vue": "3.2.0",
        "@vitejs/plugin-vue-jsx": "2.1.1",
        "@vue/eslint-config-prettier": "7.0.0",
        "autoprefixer": "10.4.2",
        "commitizen": "4.2.4",
        "cssnano": "5.1.14",
        "cz-conventional-changelog": "3.3.0",
        "cz-customizable": "6.3.0",
        "eslint": "8.8.0",
        "eslint-define-config": "1.3.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-vue": "9.7.0",
        "gupo-deploy": "1.1.0",
        "husky": "7.0.4",
        "less": "4.1.2",
        "lint-staged": "11.1.2",
        "postcss": "8.4.18",
        "postcss-html": "1.5.0",
        "postcss-import": "15.0.0",
        "postcss-less": "6.0.0",
        "prettier": "2.6.1",
        "rimraf": "3.0.2",
        "rollup-plugin-visualizer": "5.8.3",
        "stylelint": "14.15.0",
        "stylelint-config-html": "1.0.0",
        "stylelint-config-prettier": "9.0.3",
        "stylelint-config-recommended": "9.0.0",
        "stylelint-config-standard": "29.0.0",
        "stylelint-order": "5.0.0",
        "tailwindcss": "3.2.4",
        "unplugin-auto-import": "0.11.4",
        "unplugin-vue-components": "0.22.9",
        "vite": "3.1.8",
        "vite-plugin-compression": "0.5.1",
        "vite-plugin-html": "3.2.0",
        "vite-plugin-mkcert": "1.10.1",
        "vite-plugin-svg-icons": "2.0.1",
        "vite-plugin-vue-setup-extend": "0.4.0",
        "vue-eslint-parser": "9.1.0"
    },
    "resolutions": {
        "bin-wrapper": "npm:bin-wrapper-china"
    }
}
